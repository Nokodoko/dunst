# See dunst(5) for all configuration options

[global]
    monitor = 0
    #follow = keyboard
    width = 300
    height = 300
    origin = top-center
    offset = 10x50
    scale = 1
    notification_limit = 0
    progress_bar = true
    progress_bar_height = 10
    progress_bar_frame_width = 1
    progress_bar_min_width = 150
    progress_bar_max_width = 300
    indicate_hidden = yes
    transparency = 30
    separator_height = 2
    padding = 2
    horizontal_padding = 2
    text_icon_padding = 0
    frame_width = 0
    close = ctrl+space
    frame_color = "#0087ff"
    separator_color = frame
    sort = yes
    font = VictorMono 11
    close = ctrl+space
    line_height = 0
    markup = full
    format = "<b>%s</b>\n%b"
    alignment = center
    vertical_alignment = center
    show_age_threshold = 60
    ellipsize = middle
    ignore_newline = no
    stack_duplicates = true
    hide_duplicate_count = false
    show_indicators = yes
    icon_position = top
    min_icon_size = 0
    max_icon_size = 32
    icon_path = /usr/share/icons/gnome/16x16/status/:/usr/share/icons/gnome/16x16/devices/:~/Pictures/
    sticky_history = yes
    history_length = 20
    dmenu= dmenu -m 0 -fn VictorMono:size=20 -nf green -nb black -nf green -sb blue -p dunst
    mouse_left_click = context,close_current
    always_run_script = true
    title = Dunst
    class = Dunst
    corner_radius = 4
    ignore_dbusclose = false
    force_xwayland = false
    force_xinerama = false
    mouse_left_click = context,close_current
    mouse_middle_click = do_action, close_current
    close_all = ctrl+shift+space
    context = mod1+Right
    enable_posix_regex = true

[urgency_low]
    # IMPORTANT: colors have to be defined in quotation marks.j
    # Otherwise the "#" and following would be interpreted as a comment.
    background = "#000000"
    foreground = "#ffffff"
    timeout = 3
    # Icon for notifications with low urgency, uncomment to enable
    new_icon = "~/Pictures/kirinTor.png"

[urgency_normal]
    background = "#000000"
    foreground = "#ffffff"
    timeout = 3
    # new_icon for notifications with normal urgency, uncomment to enable
    new_icon = "~/Pictures/artemis.png"
    #new_icon = "~/Pictures/spiderverse.jpeg"

[urgency_critical]
    background = "#900000"
    foreground = "#ffffff"
    frame_color = "#ff0000"
    timeout = 0
    # new_icon for notifications with critical urgency, uncomment to enable
    new_icon = "~/Pictures/dpWarn3.jpg"

